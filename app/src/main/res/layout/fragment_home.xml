<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >

  <com.getbase.floatingactionbutton.FloatingActionsMenu
      android:id="@+id/floatingActions"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginBottom="16dp"
      android:layout_marginEnd="16dp"
      app:fab_addButtonColorNormal="@color/colorPrimary"
      app:fab_addButtonColorPressed="@color/colorPrimaryDark"
      app:fab_labelStyle="@style/FabLabelStyle"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      >

    <com.getbase.floatingactionbutton.FloatingActionButton
        android:id="@+id/fabScan"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:fab_colorNormal="@color/colorPrimary"
        app:fab_colorPressed="@color/colorPrimaryDark"
        app:fab_icon="@drawable/ic_scan_qrcode_white_24dp"
        app:fab_title="@string/scan"
        />

    <com.getbase.floatingactionbutton.FloatingActionButton
        android:id="@+id/fabQrCode"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:fab_colorNormal="@color/colorPrimary"
        app:fab_colorPressed="@color/colorPrimaryDark"
        app:fab_icon="@drawable/ic_qrcode_white_24dp"
        app:fab_title="@string/qr_code"
        />

  </com.getbase.floatingactionbutton.FloatingActionsMenu>

</android.support.constraint.ConstraintLayout>